function createTwitchOfflineElement (data) {
  const html = `
    <img src="${data.logo}" height=100>
    <div class="twitchElement__info">
       <h3>${data.display_name}</h3>
       <a href="${data.url}"><i class="fa fa-twitch" aria-hidden="true"></i></a>

       <p>${data.bio}</p>
      </div>`


   const el = document.createElement('div')
   el.setAttribute('class', 'twitchElement')
   el.setAttribute('data-isOnline', isOnline)
   el.innerHTML = html
   return el
}


